    
extends ../layout

block content
    h1= title
    table.user_table
        tr
            th First Name
            th Last Name
            th Email
            th Username
            th Year of Birth
        if user_list && user_list.length > 0
            each user in user_list
                tr
                    td #{user.first_name}
                    td #{user.last_name}
                    td #{user.email}
                    td #{user.username}
                    td #{user.birthyear}
        else
            tr
                td(colspan=5) No records